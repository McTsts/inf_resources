{
	"multipart": [
		{
			"apply": {
				"model": "inf:machine/gate/junction/base"
			}
		},
		{
			"when": {
				"north": "true"
			},
			"apply": {
				"model": "inf:machine/gate/junction/output_side"
			}
		},
		{
			"when": {
				"east": "true"
			},
			"apply": {
				"model": "inf:machine/gate/junction/output_side",
				"y": 90
			}
		},
		{
			"when": {
				"south": "true"
			},
			"apply": {
				"model": "inf:machine/gate/junction/output_side",
				"y": 180
			}
		},
		{
			"when": {
				"west": "true"
			},
			"apply": {
				"model": "inf:machine/gate/junction/output_side",
				"y": 270
			}
		},
		{
			"when": {
				"up": "true"
			},
			"apply": {
				"model": "inf:machine/gate/junction/output_top",
				"x": 0
			}
		},
		{
			"when": {
				"down": "true"
			},
			"apply": {
				"model": "inf:machine/gate/junction/output_top",
				"x": 180
			}
		},
		{
			"when": {
				"north": "false"
			},
			"apply": {
				"model": "inf:machine/gate/junction/input_side"
			}
		},
		{
			"when": {
				"east": "false"
			},
			"apply": {
				"model": "inf:machine/gate/junction/input_side",
				"y": 90
			}
		},
		{
			"when": {
				"south": "false"
			},
			"apply": {
				"model": "inf:machine/gate/junction/input_side",
				"y": 180
			}
		},
		{
			"when": {
				"west": "false"
			},
			"apply": {
				"model": "inf:machine/gate/junction/input_side",
				"y": 270
			}
		},
		{
			"when": {
				"up": "false"
			},
			"apply": {
				"model": "inf:machine/gate/junction/input_top",
				"x": 0
			}
		},
		{
			"when": {
				"down": "false"
			},
			"apply": {
				"model": "inf:machine/gate/junction/input_top",
				"x": 180
			}
		}
	]
}